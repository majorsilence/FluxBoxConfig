[begin] (fluxbox)
[exec] (Firefox) {firefox}
[exec] (Lock Screen) {gnome-screensaver-command -l}
[exec] (Terminal) {gnome-terminal}
[exec] (Sublime) {subl}
[exec] (Monodevelop) {monodevelop}
[exec] (File Manager - xfe) {xfe}
[exec] (File Manager - nautilus) {nautilus}
[exec] (Music) {clementine}
[exec] (Torrents) {transmission-gtk}
[exec] (Suspend) {dbus-send --system --print-reply --dest=org.freedesktop.UPower /org/freedesktop/UPower org.freedesktop.UPower.Suspend ; gnome-screensaver-command -l}
